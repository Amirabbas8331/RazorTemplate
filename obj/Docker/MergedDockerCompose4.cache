name: dockercompose17900555484498651779
services:
  es01:
    container_name: es01
    environment:
      ELASTIC_PASSWORD: MyStrongPassword123!
      discovery.type: single-node
      xpack.security.enabled: "true"
      xpack.security.http.ssl.enabled: "false"
    image: docker.elastic.co/elasticsearch/elasticsearch:9.1.5
    networks:
      elastic: null
    ports:
      - mode: ingress
        target: 9200
        published: "9200"
        protocol: tcp
  fleet01:
    container_name: fleet01
    depends_on:
      es01:
        condition: service_started
        required: true
      kib01:
        condition: service_started
        required: true
    environment:
      FLEET_ENROLLMENT_TOKEN: ""
      FLEET_SERVER_ELASTICSEARCH_HOST: http://es01:9200
      FLEET_SERVER_ENABLE: "true"
      KIBANA_HOST: http://kib01:5601
    image: elastic/elastic-agent:latest
    networks:
      elastic: null
    ports:
      - mode: ingress
        target: 8220
        published: "8220"
        protocol: tcp
  kib01:
    container_name: kib01
    depends_on:
      es01:
        condition: service_started
        required: true
    environment:
      ELASTICSEARCH_HOSTS: http://es01:9200
      ELASTICSEARCH_SERVICEACCOUNT_TOKEN: ""
      xpack.encryptedSavedObjects.encryptionKey: f26a3e85d06e483a83f36ed7f9848b53
      xpack.reporting.encryptionKey: 2013e2808ed8482b822c1f9adcb5b193
      xpack.security.encryptionKey: fdca3d408d224e76bc4cea08c9f906fa
    image: docker.elastic.co/kibana/kibana:9.1.5
    networks:
      elastic: null
    ports:
      - mode: ingress
        target: 5601
        published: "5601"
        protocol: tcp
  razortemplate:
    build:
      context: E:\Repos\RazorTemplate
      dockerfile: RazorTemplate/Dockerfile
    container_name: razortemplate
    depends_on:
      es01:
        condition: service_started
        required: true
      kib01:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: razortemplate
    networks:
      elastic: null
    ports:
      - mode: ingress
        target: 80
        published: "8080"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
